@model Plan.Models.StatusNextMaster

@{
    ViewData["Title"] = "Edit Status Next Master";
}



<form asp-action="Edit">
  @Html.AntiForgeryToken()
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="StatusNext" class="control-label"></label>
        <input asp-for="StatusNext" class="form-control" />
        <span asp-validation-for="StatusNext" class="text-danger"></span>
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Update</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
