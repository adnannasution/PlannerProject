@model Plan.Models.JenisKontrakMaster

@{
    ViewData["Title"] = "Jenis kontrak Master";

    ViewData["Title1"] = "Jenis Kontrak Master";
    ViewData["Title2"] = "Create";

}

<form asp-action="Create">
  @Html.AntiForgeryToken()
    <div class="form-group">
        <label asp-for="Jenis" class="control-label"></label>
        <input asp-for="Jenis" class="form-control" required/>
